    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite-loader": "^5.0.0",
    "svg-transform-loader": "^2.0.13",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "taskkit-svg-sprite": "^3.1.1",
    "webpack": "^4.44.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.2.0",
    "webpack-fix-style-only-entries": "^0.5.2"
