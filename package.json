{
  "name": "clientkit",
  "version": "1.0.1",
  "description": "Client kit for websites",
  "main": "index.js",
  "bin": {
    "clientkit": "index.js"
  },
  "scripts": {
    "start": "node index.js --config conf --env ck",
    "dev": "concurrently 'npm run dev:ck' 'npm run serve'",
    "dev:ck": "node index.js --config demo/clientkit",
    "test": "mocha",
    "serve": "serve -p 8082 dist",
    "ghpages": "./tasks/ghpages",
    "dockerhub": "./tasks/docker-push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/clientkit.git"
  },
  "author": "First+Third",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/clientkit/issues"
  },
  "homepage": "https://github.com/firstandthird/clientkit#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "concurrently": "2.2.0",
    "mocha": "2.5.3",
    "serve": "1.4.0",
    "eslint-config-firstandthird": "3.1.0",
    "eslint-plugin-import": "2.2.0"
  },
  "dependencies": {
    "async": "2.1.4",
    "clientkit-css": "0.0.6",
    "mkdirp": "0.5.1",
    "purdy": "^2.2.0",
    "rmdir": "^1.2.0",
    "taskkit": "1.0.0",
    "taskkit-eslint": "0.0.3",
    "taskkit-hash": "0.0.4",
    "taskkit-rollup": "0.0.8",
    "taskkit-task": "0.0.13",
    "taskkit-watcher": "0.0.5",
    "update-notifier": "^1.0.3"
  },
  "eslintConfig": {
    "extends": "firstandthird"
  }
}
