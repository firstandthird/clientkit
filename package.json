{
  "name": "clientkit",
  "version": "6.3.0",
  "description": "Client kit for websites",
  "main": "index.js",
  "bin": {
    "clientkit": "index.js"
  },
  "scripts": {
    "start": "cd demo && node ../index.js",
    "dev": "cd demo && node ../index.js dev",
    "build": "rimraf demo/dist/* && cd demo && node ../index.js prod",
    "test": "cd test && node ../index.js && cd .. && tap -J -Rmin ./test/*.test.js",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/clientkit.git"
  },
  "author": "First+Third",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/clientkit/issues"
  },
  "homepage": "https://github.com/firstandthird/clientkit#readme",
  "dependencies": {
    "@alaguna/css-mqpacker": "^8.0.1",
    "@alaguna/postcss-font-magician": "^2.2.1",
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "async": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "chokidar": "^3.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "clientkit-styleguide": "^2.0.0",
    "confi": "^10.0.1",
    "css-loader": "^5.1.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-config-firstandthird": "^6.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "merge-options": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "normalize.css": "^8.0.1",
    "path-exists": "^4.0.0",
    "postcss": "^7.0.35",
    "postcss-calc": "^7.0.5",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-properties": "^9.2.0",
    "postcss-easings": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^4.1.0",
    "postcss-loader": "^5.2.0",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-svgo": "^4.0.2",
    "postcss-triangle": "^2.0.0",
    "require-all": "^3.0.0",
    "sort-css-media-queries": "^1.5.4",
    "style-loader": "^2.0.0",
    "stylelint-config-firstandthird": "^1.2.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite-loader": "^6.0.2",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "svg-transform-loader": "^2.0.13",
    "svgo": "^2.2.2",
    "svgo-loader": "^3.0.0",
    "taskkit-svg-sprite": "^3.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.27.1",
    "webpack-assets-manifest": "^5.0.1",
    "webpack-remove-empty-scripts": "^0.7.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "tap": "^14.11.0"
  },
  "eslintConfig": {
    "extends": "firstandthird"
  },
  "stylelint": {
    "extends": "stylelint-config-firstandthird",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "define-mixin",
            "mixin"
          ]
        }
      ]
    }
  }
}
