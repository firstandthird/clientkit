{
  "name": "clientkit",
  "version": "0.6.0",
  "description": "Client kit for websites",
  "main": "app.js",
  "bin": {
    "clientkit": "app.js"
  },
  "scripts": {
    "start": "node app.js --config conf",
    "dev": "node app.js dev",
    "test": "mocha",
    "example:build": "node app.js --config $PWD/examples/clientkit",
    "example:dev": "node app.js dev --config $PWD/examples/clientkit",
    "example:serve": "serve -p 8082 examples",
    "example": "concurrently \"npm run example:dev\" \"npm run example:serve\"",
    "ghpages": "npm run example:build && node tasks/ghpages.js",
    "dockerhub": "./tasks/docker-push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/clientkit.git"
  },
  "author": "First+Third",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/firstandthird/clientkit/issues"
  },
  "homepage": "https://github.com/firstandthird/clientkit#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "gh-pages": "^0.11.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.4",
    "serve": "^1.4.0",
    "stylelint": "^6.7.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chokidar": "^1.6.0",
    "confi": "^2.3.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.7.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-firstandthird": "^2.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "exorcist": "^0.4.0",
    "get-stdin": "^5.0.1",
    "lodash.capitalize": "^4.2.0",
    "lodash.debounce": "^4.0.6",
    "lodash.reduce": "^4.5.1",
    "logr": "^1.11.0",
    "mkdirp": "^0.5.1",
    "normalize.css": "^4.1.1",
    "path-exists": "^3.0.0",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.6.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^8.1.2",
    "postcss-inline-svg": "^2.0.1",
    "postcss-mixins": "^4.0.2",
    "postcss-svgo": "^2.1.3",
    "postcss-triangle": "^1.0.1",
    "require-all": "^2.0.0",
    "uglifyify": "^3.0.2",
    "yargs": "^4.8.1"
  }
}
