{
  "name": "clientkit",
  "version": "1.0.0-alpha.7",
  "description": "Client kit for websites",
  "main": "app.js",
  "bin": {
    "clientkit": "app.js"
  },
  "scripts": {
    "start": "node app.js --config conf --env ck",
    "dev": "concurrently 'npm run dev:ck' 'npm run serve'",
